   "outputs": [],
   "source": [
    "# Extract only the relevant columns for analysis\n",
    "# In this case:\n",
    "#    - '10th Mark' refers to the student's 10th-grade grades on a 0-100 scale.\n",
    "#    - '12th Mark' refers to the student's 12th-grade grades on a 0-100 scale.\n",
    "#    - 'college mark' refers to the student's college grades on a 0-100 scale.\n",
    "#    - 'daily studing time' refers to the student's approximate average time spent studying in a day,\n",
    "#              formatted as: 1 - 30 minute, 30 - 60 min, 1 - 2 Hour, 2 - 3 hour, 3 - 4 hour\n",
    "#    - 'Do you like your degree?' refers to whether or not a student likes their degree,\n",
    "#              formatted as: Yes, No\n",
    "#    - 'Travelling Time' refers to the student's approximate average time travelling/commuting in a day,\n",
    "#              formatted as: 0 - 30 minutes, 30 - 60 minutes, 1 - 1.30 hour, 1.30 - 2 hour, 2 - 2.30 hour\n",
    "#    - 'Stress Level' refers to the student's self-proclaimed stress level,\n",
    "#              formatted as: Good, Bad, Awful, fabulous\n",
    "\n",
    "studentBehavior = studentBehavior[['10th Mark','12th Mark','college mark','daily studing time','Do you like your degree?','Travelling Time ','Stress Level ']]\n",
    "studentBehavior.shape()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "e4bcbd91",
   "metadata": {},
   "outputs": [],
   "source": [
    "#For consistency, rename columns to capitialized columns with correct spelling and no extra spaces.\n",
    "\n",
    "studentBehavior = studentBehavior.rename(columns={\"college mark\": \"College Mark\", \"daily studing time\": \"Daily Studying Time\", \"Travelling Time \": \"Travelling Time\", \"Stress Level \": \"Stress Level\"})"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "69d22b28",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Standardize time blocks by the following partitions:\n",
    "#    < 30mins , 30-60mins, 1-1.5hrs , 1.5-2hrs , > 2hrs, 1-2hrs\n",
    "\n",
    "def standardize_time(str_in):\n",
    "    \n",
    "    try:\n",
    "\n",
    "        if '0 - 30 minute' in str_in or '0 - 30 minutes' in str_in:\n",
    "            str_in = str_in.replace('0 - 30 minute', '< 30mins')\n",
    "            output = str_in\n",
    "    \n",
    "        elif '30 - 60 minute' in str_in or '30 - 60 minutes' in str_in:\n",
    "            str_in = str_in.replace('30 - 60 minute', '30-60mins')\n",
    "            output = str_in\n",
    "                \n",
    "        elif '1 - 2 Hour' in str_in:\n",
    "            str_in = str_in.replace('1 - 2 Hour', '1-2hrs')\n",
    "            output = str_in\n",
    "        \n",
    "        elif '1 - 1.30 hour' in str_in:\n",
    "            str_in = str_in.replace('1 - 1.30 hour', '1-1.5hrs')\n",
    "            output = str_in\n",
    "        \n",
    "        elif '1.30 - 2 hour' in str_in:\n",
    "            str_in = str_in.replace('1.30 - 2 hour', '1.5-2hrs')\n",
    "            output = str_in\n",
    "\n",
    "        elif '2 - 3 hour' in str_in or '3 - 4 hour' in str_in or '2 - 2.30 hour' in str_in:\n",
    "            str_in = str_in.replace('2 - 3 hour', '> 2hrs')\n",
    "            str_in = str_in.replace('3 - 4 hour', '> 2hrs')\n",
    "            str_in = str_in.replace('2 - 2.30 hour', '> 2hrs')\n",
    "            output = str_in\n",
    "        else:\n",
    "            output = np.nan\n",
    "    except:\n",
    "        output = np.nan\n",
    "        \n",
    "    return output\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "aa44b211",
   "metadata": {},
   "outputs": [],
   "source": [
    "studentBehavior['Daily Studying Time'] = studentBehavior['Daily Studying Time'].apply(standardize_time)\n",
    "\n",
    "studentBehavior['Travelling Time'] = studentBehavior['Travelling Time'].apply(standardize_time)\n",
    "\n",
    "studentBehavior\n"
   ]
  },
